#ifndef AGGREGATOR_H_
#define AGGREGATOR_H_

#include <iostream>
using namespace std;

#include "Sensor.h";
#include <vector>;

class Aggregator {
private:
	vector<Sensor> v;
public:
	virtual void addReading(Sensor e);
//	virtual void display();
	virtual Sensor getMinimum();
	virtual Sensor getMaximum();
	Aggregator();
	virtual ~Aggregator();

};


#endif /* AGGREGATOR_H_ */
