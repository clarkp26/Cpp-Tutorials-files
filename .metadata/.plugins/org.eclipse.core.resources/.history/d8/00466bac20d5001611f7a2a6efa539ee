#include <iostream>
using namespace std;

int* difference(int a[],int b){
	if (b < 2){
		return 0;
	}
	int *ans[] = {};
	a[b-2] = a[b-1]-a[b-2];
	for (int i=0;i<b-1;i++){
		ans[i] = a[i];
	}
	return ans;
}
int main() {
	int y[] = difference({1,2,3,4},4);
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
