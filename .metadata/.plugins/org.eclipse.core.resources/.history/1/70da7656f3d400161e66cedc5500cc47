class A{
private:
public:
	A();
};
A::A(){}
class B: public A{
private:
public:
	B();
};
B::B(){}
int main() {
	/*dynamic cast*/
	A *a = new B();
	B *c = dynamic_cast<B*>(a);
	/*static cast*/
	float y = 20.12;
	int x = static_cast<int>(y);
	cout<<x<<endl;
	return 0;
}
